{
  "expo": {
    "name": "Face Lord",
    "slug": "facelord",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/APP ICON.png",
    "scheme": "facelord",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["assets/**/*"],
    "ios": {
      "buildNumber": "0.0.3",
      "bundleIdentifier": "com.facelord.flord",
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "NSCameraUsageDescription": "This app uses the camera for personality analysis with facial features.",
        "NSPhotoLibraryUsageDescription": "This app uses the camera roll for personality analysis with facial features."
      }
    },
    "android": {
      "versionCode": 1,

      "useNextNotificationsApi": true,
      "permissions": ["CAMERA", "READ_EXTERNAL_STORAGE", ""]
    },
    "description": "Analyze your character with AI"
  }
}
